; Initialize (at the start of the round)
[Statedef 5900]
type = S

[State 5900, 1] ;Clear all int variables
type = VarRangeSet
trigger1 = RoundsExisted = 0
value = 0
first = 2
last = 15

[State 5900, 2] ;Clear all float variables
type = VarRangeSet
trigger1 = RoundsExisted = 0
fvalue = 0

;===========================================================================
; 本体関連設定 ここから
;===========================================================================
;※設定方法の説明
;value = ○ * (★)　← この記述がある場所の【★】を任意の数字に変更してください。

;限界ダメージ有無設定
[State 5900, Config_1]
type = VarAdd
trigger1 = !(Var(50) & 1)
v = 50
value = 1 * (1)		;← 限界ダメージの有無を設定できます。（ただし、ダッグ戦は機能しません。）
ignorehitpause = 1	;「コンボの合計ダメージが約7割を超えるとそれ以降はダメージが入らなくなる」
			;という原作の仕様で、これのおかげで即死が出来ないようになっています。
			;小次郎は永久もないので、有りでも無しでもあんまり変わらないのかな？
			;０ ･･･ 限界ダメージなし
			;１ ･･･ 限界ダメージあり（原作通り）		※デフォルト


;自動復帰ステート中の無敵設定
[State 5900, Config_2]
type = VarAdd
trigger1 = !(Var(50) & 2)
trigger1 = !(Var(50) & 4)
v = 50
value = 2 * (1)		;← 自動復帰ステート(State 5040)中を無敵にするかどうかを設定できます。
ignorehitpause = 1	;原作の「デフォルトで追撃不可、追撃可能技が当たった場合は専用判定が出現し追撃可能」
			;の仕様をMUGENで再現するための設定です。
			;設定を出したり消したりは出来ないので、無敵で再現してあります。
			;０ ･･･ 無敵なし
			;１ ･･･ HitOver後に無敵あり			※デフォルト
			;２ ･･･ ステート中は完全無敵


;前方ジャンプガード設定
[State 5900, Config_3]
type = VarAdd
trigger1 = !(Var(50) & 8)
v = 50
value = 8 * (1)		;← 前方ジャンプ時にガードが出来るかどうかを設定できます。
ignorehitpause = 1	;文字通りですが、原作では前方ジャンプ時ではガードが出来ない仕様になっています。
			;MUGENではデメリットになるだけなので、可能・不可能を設定できるようにー。
			;０ ･･･ 前方ジャンプ時ガード不可（原作通り）
			;１ ･･･ 前方ジャンプ時ガード可能		※デフォルト


;極剣質防御力設定
[State 5900, Config_4]
type = VarAdd
trigger1 = !(Var(50) & 16)
v = 50
value = 16 * (1)	;← 「極」剣質時での防御力を設定できます。
ignorehitpause = 1	;原作では「極」剣質になると防御力が低下します。
			;･････が、あまりにも脆くなりすぎなので緩和できる設定を設けてみました。
			;０ ･･･ 防御力低下あり（原作通り）
			;１ ･･･ 防御力低下なし				※デフォルト


;極剣質ゲージ回収率設定
[State 5900, Config_5]
type = VarAdd
trigger1 = !(Var(50) & 32)
v = 50
value = 32 * (1)	;← 「極」剣質時でのゲージ回収率を設定できます。
ignorehitpause = 1	;こちらも原作の「極」剣質だと、極端にゲージが回収できなくなるので･･･。
			;設定をＯＮにすると、攻撃をガードされた時の回収率が少しだけ緩和されます。
			;０ ･･･ ゲージ回収率増加なし（原作通り）
			;１ ･･･ ゲージ回収率増加あり			※デフォルト


;ガード硬直短縮設定
[State 5900, Config_6]
type = VarAdd
trigger1 = !(Var(50) & 64)
v = 50
value = 64 * (0)	;← 原作での最重要テクニック、ガード硬直短縮が出来るかどうかを設定できます。
ignorehitpause = 1	;ガードの硬直中に「しゃがみガード → 立ちガード」または「立ちガード → しゃがみガード」
			;と切り替えると、ガード時の硬直時間が短縮されます。
			;０ ･･･ ガード硬直短縮不可			※デフォルト
			;１ ･･･ ガード硬直短縮可能（原作通り）


